{
  "dashboard": {
    "title": "Strategy Generator AI - Performance",
    "tags": ["quantum-trader", "strategy-generator"],
    "timezone": "browser",
    "panels": [
      {
        "title": "Strategy Status Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "strategy_generator_strategies_by_status"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "title": "Generation Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(strategy_generator_generations_total[5m])"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "title": "Average Fitness Score",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, strategy_generator_fitness_score_bucket)"
          },
          {
            "expr": "histogram_quantile(0.95, strategy_generator_fitness_score_bucket)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "title": "Average Profit Factor",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, strategy_generator_profit_factor_bucket)"
          },
          {
            "expr": "histogram_quantile(0.95, strategy_generator_profit_factor_bucket)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "title": "Generation Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, strategy_generator_generation_duration_seconds_bucket)"
          },
          {
            "expr": "histogram_quantile(0.95, strategy_generator_generation_duration_seconds_bucket)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "title": "Shadow Test Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(strategy_generator_shadow_tests_total[5m]) - rate(strategy_generator_shadow_test_errors_total[5m])"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "title": "Active LIVE Strategies",
        "type": "stat",
        "targets": [
          {
            "expr": "strategy_generator_active_live_strategies"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24}
      },
      {
        "title": "Strategies Deployed (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(strategy_generator_strategies_deployed_total[24h])"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24}
      },
      {
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(strategy_generator_generation_errors_total[5m])"
          },
          {
            "expr": "rate(strategy_generator_shadow_test_errors_total[5m])"
          },
          {
            "expr": "rate(strategy_generator_deployment_errors_total[5m])"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [0.05],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "name": "High Error Rate Alert"
        }
      }
    ],
    "refresh": "30s",
    "schemaVersion": 16,
    "version": 0
  }
}
