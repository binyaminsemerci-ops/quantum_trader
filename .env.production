# Production Environment Configuration
# Copy this file to .env for production deployment

# ============================================================================
# ENVIRONMENT
# ============================================================================
QT_ENV=production
LOG_LEVEL=INFO
QT_LOG_LEVEL=INFO

# ============================================================================
# BINANCE API (PRODUCTION)
# ============================================================================
BINANCE_API_KEY=your_production_api_key_here
BINANCE_API_SECRET=your_production_api_secret_here
BINANCE_TESTNET=false

# ============================================================================
# AI MODEL SELECTION
# ============================================================================
AI_MODEL=hybrid

# ============================================================================
# TRADING CONFIGURATION (CONSERVATIVE FOR PRODUCTION)
# ============================================================================
QT_EVENT_DRIVEN_MODE=true
QT_CONFIDENCE_THRESHOLD=0.55
QT_CHECK_INTERVAL=10
QT_COOLDOWN_SECONDS=180

# Orchestrator Policy (Conservative)
QT_POLICY_MIN_CONF_TRENDING=0.28
QT_POLICY_MIN_CONF_RANGING=0.35
QT_POLICY_MIN_CONF_NORMAL=0.30

# ============================================================================
# RISK MANAGEMENT (PRODUCTION - CONSERVATIVE)
# ============================================================================
RM_MAX_POSITION_USD=1500
RM_MIN_POSITION_USD=100
RM_MAX_LEVERAGE=20.0
RM_RISK_PER_TRADE_PCT=0.08
RM_MIN_RISK_PCT=0.01
RM_MAX_RISK_PCT=0.04
RM_MAX_EXPOSURE_PCT=1.50
RM_HIGH_CONF_MULT=1.5
RM_LOW_CONF_MULT=0.5
RM_SIGNAL_QUALITY_ADJ=true
RM_MAX_CONCURRENT_TRADES=10

# ============================================================================
# EXECUTION
# ============================================================================
QT_PAPER_TRADING=false
STAGING_MODE=false
QT_ENABLE_EXECUTION=false
QT_ENABLE_AI_TRADING=false
QT_EXECUTION_EXCHANGE=binance-futures
QT_MARKET_TYPE=usdm_perp
QT_MIN_CONFIDENCE=0.65
QT_EXECUTION_MIN_NOTIONAL=20.0
QT_QUOTES=USDT
DEFAULT_QUOTE=USDT

# ============================================================================
# UNIVERSE SELECTION (TOP 100 FOR DATA COLLECTION)
# ============================================================================
QT_UNIVERSE=l1l2-top
QT_MAX_SYMBOLS=100
QT_ALLOWED_SYMBOLS=BTCUSDT,ETHUSDT,SOLUSDT,XRPUSDT,DOGEUSDT,BNBUSDT,AVAXUSDT,LINKUSDT,ADAUSDT,DOTUSDT,MATICUSDT,UNIUSDT,LTCUSDT,BCHUSDT,FILUSDT,ICPUSDT,NEARUSDT,AAVEUSDT,FETUSDT,TRXUSDT,ATOMUSDT,APTUSDT,OPUSDT,ARBUSDT,SUIUSDT,SEIUSDT,INJUSDT,STXUSDT,RENDERUSDT,TIAUSDT

# ============================================================================
# AI FEATURES
# ============================================================================
QT_USE_AI_DYNAMIC_TPSL=true
QT_FORCE_EXITS_ENABLED=true

# Model Supervisor (Enforced)
QT_MODEL_SUPERVISOR_MODE=ENFORCED
QT_MODEL_SUPERVISOR_BIAS_THRESHOLD=0.70
QT_MODEL_SUPERVISOR_MIN_SAMPLES=20

# RL Position Sizing (Production)
RL_SIZING_EPSILON=0.20
RL_SIZING_ALPHA=0.10
RL_SIZING_DISCOUNT=0.95

# ============================================================================
# CONTINUOUS LEARNING (PRODUCTION SCHEDULE)
# ============================================================================
QT_CONTINUOUS_LEARNING=true
QT_MIN_SAMPLES_FOR_RETRAIN=100
QT_RETRAIN_INTERVAL_HOURS=48
QT_AUTO_BACKTEST_AFTER_TRAIN=true

# ============================================================================
# MONITORING
# ============================================================================
QT_MODEL_SUPERVISOR_INTERVAL=300
QT_BIAS_THRESHOLD=0.65
QT_PORTFOLIO_BALANCER_INTERVAL=60
QT_MAX_CORRELATION=0.7

# Production Hardening
UVICORN_WORKERS=1
QT_ENABLE_METRICS=true
QT_ENABLE_HEALTH_CHECKS=true
QT_ENABLE_CRASH_RECOVERY=true

# ============================================================================
# REDIS (ARCHITECTURE V2)
# ============================================================================
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_DB=0

# ============================================================================
# OPTIONAL: GRAFANA
# ============================================================================
GRAFANA_ADMIN_PASSWORD=change_me_in_production
