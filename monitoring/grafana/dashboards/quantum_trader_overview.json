{
  "dashboard": {
    "title": "Quantum Trader - System Overview",
    "tags": ["quantum_trader", "overview"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "AI Engine - Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"ai-engine\"}",
            "legendFormat": "AI Engine Status"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Execution Service - Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"execution\"}",
            "legendFormat": "Execution Status"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Request Rate (req/min)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[1m]) * 60",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 4,
        "title": "Response Time (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      }
    ]
  }
}
