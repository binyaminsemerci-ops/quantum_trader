{
  "dashboard": {
    "title": "Quantum Trader - Service Overview",
    "tags": ["quantum-trader", "observability", "epic-obs-001"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(http_requests_total, service)",
          "multi": true,
          "includeAll": true,
          "refresh": 1
        },
        {
          "name": "environment",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(http_requests_total, environment)",
          "multi": false,
          "includeAll": false,
          "refresh": 1
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Traffic",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (service) (rate(http_requests_total{service=~\"$service\", environment=\"$environment\"}[5m]))",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Error Rate (%)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (service) (rate(http_requests_total{service=~\"$service\", environment=\"$environment\", status=~\"5..\"}[5m])) / sum by (service) (rate(http_requests_total{service=~\"$service\", environment=\"$environment\"}[5m])) * 100",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Error Rate"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [5], "type": "gt"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "frequency": "60s",
          "handler": 1,
          "name": "High Error Rate",
          "message": "Error rate > 5%"
        }
      },
      {
        "id": 3,
        "title": "Latency (P95)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (service, le) (rate(http_request_duration_seconds_bucket{service=~\"$service\", environment=\"$environment\"}[5m])))",
            "legendFormat": "{{service}} P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum by (service, le) (rate(http_request_duration_seconds_bucket{service=~\"$service\", environment=\"$environment\"}[5m])))",
            "legendFormat": "{{service}} P50",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Signals Processed",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (service) (rate(signals_generated_total{service=\"ai-engine\", environment=\"$environment\"}[5m]))",
            "legendFormat": "Signals Generated",
            "refId": "A"
          },
          {
            "expr": "sum by (service) (rate(signals_rejected_total{service=\"ai-engine\", environment=\"$environment\"}[5m]))",
            "legendFormat": "Signals Rejected",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Signals/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Service Health Status",
        "type": "stat",
        "gridPos": {"x": 0, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "up{job=~\"ai-engine|execution-service|risk-service|portfolio-service\", environment=\"$environment\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "orientation": "auto",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"type": "value", "value": "1", "text": "UP"},
              {"type": "value", "value": "0", "text": "DOWN"}
            ]
          }
        }
      },
      {
        "id": 6,
        "title": "Trades Executed (Last Hour)",
        "type": "stat",
        "gridPos": {"x": 6, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(increase(trades_executed_total{environment=\"$environment\"}[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "green", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Open Positions",
        "type": "stat",
        "gridPos": {"x": 12, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "portfolio_open_positions_total{environment=\"$environment\"}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "orientation": "auto",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 30},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Portfolio PnL (%)",
        "type": "stat",
        "gridPos": {"x": 18, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "portfolio_pnl_percent{environment=\"$environment\"}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": -5},
                {"color": "yellow", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      }
    ],
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  },
  "overwrite": false,
  "folderId": 0,
  "message": "EPIC-OBS-001 Phase 5: Initial dashboard template"
}
