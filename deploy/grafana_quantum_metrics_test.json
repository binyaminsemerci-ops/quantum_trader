{
  "dashboard": {
    "title": "Quantum Metrics Test (Working)",
    "tags": ["quantum", "test"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "RL Shadow - Pass Rate % (All Symbols)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "avg(quantum_rl_gate_pass_rate) * 100",
            "refId": "A",
            "legendFormat": "Avg Pass Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 2,
        "title": "RL Shadow - Total Intents Analyzed",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(quantum_rl_intents_analyzed_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "RL Shadow - Eligible Rate %",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "avg(quantum_rl_eligible_rate) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 4,
        "title": "Safety - Safe Mode Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "quantum_safety_safe_mode",
            "refId": "A"
          }
        ],
        "options": {
          "textMode": "value_and_name",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "OFF", "color": "green"},
              {"type": "value", "value": "1", "text": "ON", "color": "red"}
            ]
          }
        }
      },
      {
        "id": 5,
        "title": "Safety - TTL Seconds",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 4},
        "targets": [
          {
            "expr": "quantum_safety_safe_mode_ttl_seconds",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 6,
        "title": "Redis - Clients Connected",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "redis_connected_clients",
            "refId": "A",
            "legendFormat": "Clients"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 7,
        "title": "Redis - Memory Used (MB)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "redis_memory_used_bytes / 1024 / 1024",
            "refId": "A",
            "legendFormat": "Memory MB"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decmbytes"
          }
        }
      },
      {
        "id": 8,
        "title": "Harvest - Kill Score by Symbol (Top 10)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "topk(10, quantum_harvest_kill_score)",
            "refId": "A",
            "legendFormat": "{{symbol}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 9,
        "title": "Safety - Faults Last Hour",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "quantum_safety_faults_last_1h",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Safety - Trade Intent Rate (per min)",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
        "targets": [
          {
            "expr": "quantum_trade_intent_rate_per_min",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "rpm",
            "min": 0,
            "max": 60
          }
        }
      },
      {
        "id": 11,
        "title": "Node Exporter - CPU Usage %",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "100 - (avg(irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Quantum Services - Active Count",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 24},
        "targets": [
          {
            "expr": "count(node_systemd_unit_state{name=~\"quantum.*\",state=\"active\"})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      }
    ]
  },
  "overwrite": true
}
