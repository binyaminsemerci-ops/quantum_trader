# Frontend Environment Variables Configuration

# API Base URL - Change this to your backend URL
VITE_API_BASE_URL=http://localhost:8000

# WebSocket URL for real-time updates
VITE_WS_URL=ws://localhost:8000/ws

# Enable development mode features
VITE_DEV_MODE=true

# Trading demo mode (uses mock data when true)
VITE_DEMO_MODE=false

# Example placeholders (replace in real .env, do not commit secrets)
VITE_PUBLIC_API_KEY=your_public_key_here # pragma: allowlist secret
VITE_FEATURE_FLAG_EXPERIMENTAL=false



