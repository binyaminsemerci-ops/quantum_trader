import { useState } from 'react';

export default function ApiTest() {
  const [result, setResult] = useState('');
  const call = async () => {
    const res = await fetch('/api/test');
    const data = await res.text();
    setResult(data);
  };
  return (
    <div>
      <button onClick={call}>Call API</button>
      <pre>{result}</pre>
    </div>
  );
}
