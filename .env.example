# Example environment variables (do NOT commit real secrets)
# Copy this file to .env and fill in values for local development.

# Binance API credentials
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here

# CryptoPanic API key
CRYPTOPANIC_KEY=your_cryptopanic_key_here

# Twitter / X developer keys (optional)
X_API_KEY=your_x_api_key_here
X_API_SECRET=your_x_api_secret_here
X_BEARER_TOKEN=your_x_bearer_token_here
# === Backend Config ===
PYTHONUNBUFFERED=1

# === Database ===
DB_URL=sqlite:///./trades.db

# === Binance API Keys (sett dine egne ved bruk) ===
BINANCE_API_KEY=dummy
BINANCE_API_SECRET=dummy

# === Risk Management Defaults ===
DEFAULT_BALANCE=10000
DEFAULT_RISK_PERCENT=1.0
