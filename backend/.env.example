# Backend environment example
# Copy to backend/.env and fill in real values (do NOT commit real secrets)
```bash
# Backend environment example
# Copy to backend/.env and fill in real values (do NOT commit real secrets)

# NOTE: The project runs standalone using a local SQLite database by default.
# You do not need to provide usernames or external DB credentials for local
# development or for running the bundled test-suite. If you prefer Postgres,
# set DB_USER, DB_PASS and DATABASE_URL in your local `backend/.env`.

DB_USER=your_db_username
DB_PASS=your_db_password

# Default to a local SQLite file for standalone runs (no external credentials required):
DATABASE_URL=sqlite:///./db.sqlite3

# Optional: set a SECRET_KEY for session signing / cryptography (do not commit real secrets)
SECRET_KEY=changeme

# Optional external service keys (leave empty for local runs)
SAFETY_API_KEY=
DEFAULT_EXCHANGE=binance
BINANCE_API_KEY=qy5g2Dxa8JyNzWI29Ub9ZBbaEWAbTjTz91OMCOWWBd7LRlrnLSO82uQFLuMWGoHb
BINANCE_API_SECRET=oIcNvT3IzlnDwrPxor8jxmCnt2f7ClmmreLsfog2R1QidbgBXNCvm0KyWcF1ebbg

# Scheduler configuration
QUANTUM_TRADER_REFRESH_SECONDS=180
QUANTUM_TRADER_SYMBOLS=BTCUSDT,ETHUSDT
# Set to 1 to disable the background scheduler (e.g. during tests)
QUANTUM_TRADER_DISABLE_SCHEDULER=
QUANTUM_TRADER_LIQUIDITY_REFRESH_SECONDS=900
QUANTUM_TRADER_EXECUTION_SECONDS=1800
# Set to 1 to disable specific scheduler jobs while keeping the scheduler active
QUANTUM_TRADER_DISABLE_LIQUIDITY=
QUANTUM_TRADER_DISABLE_EXECUTION=

# Execution engine configuration
QT_EXECUTION_MIN_NOTIONAL=50.0
QT_EXECUTION_MAX_ORDERS=10
QT_EXECUTION_CASH_BUFFER=0.0
QT_EXECUTION_ALLOW_PARTIAL=true
# Optional forced-exit controls (defaults: SL 3%, TP 5%, trailing 2%)
QT_FORCE_EXITS_ENABLED=1
# Leave the following blank to use defaults; set explicit percentages (e.g. 0.02)
QT_SL_PCT=
QT_TP_PCT=
QT_TRAIL_PCT=
QT_PARTIAL_TP=
QT_FORCE_EXIT_SL_DEFAULT=0.03
QT_FORCE_EXIT_TP_DEFAULT=0.05
QT_FORCE_EXIT_TRAIL_DEFAULT=0.02
QT_FORCE_EXIT_PARTIAL_DEFAULT=0.0

# Risk guard configuration
STAGING_MODE=0
QT_KILL_SWITCH=0
QT_MAX_NOTIONAL_PER_TRADE=1000
QT_MAX_DAILY_LOSS=500
QT_ALLOWED_SYMBOLS=BTCUSDT,ETHUSDT
QT_FAILSAFE_RESET_MINUTES=60
QT_RISK_STATE_DB=risk_state.db
# Shared secret required for /risk admin API access. Use a strong random value in
# staging/production. Leaving it blank disables the guard (local smoke testing only).
QT_ADMIN_TOKEN=

# Logging configuration
QT_LOG_LEVEL=INFO

# =============================================================================
# AUTOPILOT & DASHBOARD WARM-UP
# =============================================================================
# Toggle automatic dry-run bot startup when the backend boots (default disabled)
QT_AUTOPILOT_AUTOSTART=false
QT_AUTOPILOT_DRY_RUN=true
QT_PRICE_WARM_ENABLED=true
QT_PRICE_WARM_SYMBOLS=BTCUSDT,ETHUSDT,BNBUSDT,SOLUSDT,ADAUSDT,MATICUSDT,DOTUSDT,AVAXUSDT,LINKUSDT,UNIUSDT

```
