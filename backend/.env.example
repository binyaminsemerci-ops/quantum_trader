# Backend environment example
# Copy to backend/.env and fill in values (do NOT commit real secrets)

# Runtime database (defaults to repo-local SQLite when unset)
QUANTUM_TRADER_DATABASE_URL=sqlite:///backend/data/trades.db

# Optional: enable live market data (requires ccxt and network access)
ENABLE_LIVE_MARKET_DATA=1

# Exchange credentials (optional)
BINANCE_API_KEY=
BINANCE_API_SECRET=
COINBASE_API_KEY=
COINBASE_API_SECRET=
KUCOIN_API_KEY=
KUCOIN_API_SECRET=

# Example secret used by certain utilities
SECRET_KEY=changeme
